#include <vector>
#include <iostream>
#include <algorithm>

using namespace std;

vector<int> array = {1,2,1,2,1,2};

int unique_check(vector<int>& a){
	
	static int unique_nums = 0;
	vector<int> a2 = a;
	std::sort(a2.begin(), a2.end());
	
	if(a2.size()){
		for (int i = 0; i < a2.size(); i++){
			if (a2[i] != a2[i+1]) {unique_nums++;};
		};
		return unique_nums;
	}
	
	else {return 0;};
	
};

int main() {
	cout << "Amount of unique numbers is: " << unique_check(array);
}
